{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "standard-with-typescript",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": [2, "always"],
    "@typescript-eslint/semi": "off",
    "@typescript-eslint/indent": [2, 4],
    "indent": [2, 4]
  }
}
